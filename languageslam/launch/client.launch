<launch>
  <!--param name="/use_sim_time" value="true" /-->
  
  <param  name="robot_description"
    textfile="$(find languageslam)/urdf/linorobot.urdf" />

  <node  name="robot_state_publisher" pkg="robot_state_publisher"
    type="robot_state_publisher" />
  
  <node  name="cartographer_grpc_node" pkg="cartographer_ros"
  
      type="cartographer_grpc_node" args="
          -configuration_directory $(find languageslam)/config
          -configuration_basename client.lua
          -server_address localhost:50051
          -client_id CLIENT_ID
          "
      output="screen">
      <!--remap from="/scan" to="/1/scan"/-->
  </node>

  
</launch>

